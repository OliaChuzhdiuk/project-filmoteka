{{#each this}}
<li class="film-card">
    <img class="film-card__image" src="{{ poster_path }}" alt="{{ title  }}"/>
    <div class="film-card__description">
        <p class="film-card__title">{{ title }}</p>
        <p class="film-card__genre">{{ genres.name}} | {{ release_date }}</p>
     </div>
 </li>

{{/each}}