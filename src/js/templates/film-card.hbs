{{#each this}}
<li class="film-card">
    <img class="film-card__image" src="https://image.tmdb.org/t/p/w500/{{ poster_path }}" alt="{{ title  }}"/>
    <div class="film-card__description">
        <p class="film-card__title">{{ title }}</p>
        <p class="film-card__genre">{{ genres }} | {{ release_date }}</p>
     </div>
 </li>

{{/each}}